{
  "name": "style-object-to-css-string",
  "version": "0.0.0-development",
  "description": "A micro-package to convert style objects to css strings.  Inspired by extensive usage of styled-components.",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    },
    "./parsers": {
      "require": "./dist/parsers/index.cjs.js",
      "import": "./dist/parsers/index.es.js"
    },
    "./createParser": {
      "require": "./dist/createParser/index.cjs.js",
      "import": "./dist/createParser/index.es.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "fire": "git checkout -b emergency && git add . && git commit -m 'emergency commit' --no-verify && git push --set-upstream origin emergency --no-verify",
    "cz": "git-cz",
    "build": "vite build",
    "test": "cross-env NODE_ENV=test jest ./src",
    "test:watch": "cross-env NODE_ENV=test jest ./src --watch",
    "lint": "eslint src --ext .js .",
    "validate": "run-p lint test build",
    "release": "semantic-release"
  },
  "keywords": [
    "css",
    "styled-components"
  ],
  "author": "Tanner B. Hess Webber <tb@hesswebber.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tbhesswebber/style-object-to-css-string.git"
  },
  "bugs": {
    "url": "https://github.com/Tbhesswebber/style-object-to-css-string/issues"
  },
  "homepage": "https://github.com/Tbhesswebber/style-object-to-css-string#readme",
  "devDependencies": {
    "@commitlint/cli": "~7.5.2",
    "@commitlint/config-conventional": "~7.5.0",
    "cross-env": "~5.2.0",
    "cz-conventional-changelog": "~2.1.0",
    "eslint": "~5.3.0",
    "eslint-config-airbnb": "~17.1.0",
    "eslint-config-prettier": "~4.0.0",
    "eslint-plugin-import": "~2.16.0",
    "husky": "~1.3.1",
    "jest": "~29.7.0",
    "jest-extended": "~0.11.1",
    "npm-run-all": "~4.1.5",
    "prettier": "~1.16.4",
    "rimraf": "~2.6.3",
    "semantic-release": "^15.13.3",
    "vite": "^4.5.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run validate",
      "pre-push": "npm run validate"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "testRegex": "src/.*\\.test\\.js$",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/coverage/",
      "package.json",
      "package-lock.json"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true,
      "browser": true,
      "es6": true
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "after-used",
          "ignoreRestSiblings": false
        }
      ]
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
